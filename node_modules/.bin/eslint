#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/eslint@9.29.0_jiti@1.21.7/node_modules/eslint/bin/node_modules:/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/eslint@9.29.0_jiti@1.21.7/node_modules/eslint/node_modules:/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/eslint@9.29.0_jiti@1.21.7/node_modules:/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/eslint@9.29.0_jiti@1.21.7/node_modules/eslint/bin/node_modules:/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/eslint@9.29.0_jiti@1.21.7/node_modules/eslint/node_modules:/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/eslint@9.29.0_jiti@1.21.7/node_modules:/home/ubuntu/upload/frontend-backoffice-fature-846e3e5872267276643296c4f7059c5f81ac41a5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
